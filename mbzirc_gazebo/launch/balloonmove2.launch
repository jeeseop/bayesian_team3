<?xml version='1.0' ?>
<launch>
  <arg name='bodyname' default='balloon::link' />
  <arg name='reference_frame'  default='balloon::link' />
  <arg name='wrench' default='{ force: {x: 0, y: -0.5 z: 0} }' />
  <arg name='start_time'  default='0' />
  <arg name='duration'  default='-1' />
 
<node pkg="rosservice" type="rosservice" name="global_loc" args="call --wait /gazebo/apply_body_wrench body_name: $(arg bodyname), reference_frame: $(arg reference_frame), wrench: $(arg wrench), start_time: $(arg start_time), duration: $(arg duration)"/>
 

</launch>
